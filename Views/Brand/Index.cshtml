@model IEnumerable<Imaginary_Dealer.Models.Brand>


<br />
<br />

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Brand List</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>
<body>

    <table class="table table-striped">
        <thead class="thead-info">
            <tr>
                <th>Manage Brand Name</th>
                <th class="text-right">
                    <a class="btn btn-info btn-sm" asp-action="Create"><i class="fa-solid fa-motorcycle">&nbsp; ADD BRAND  </i></a>
                </th>
            </tr>
        </thead>
    </table>

    <div class="col-md-12">
        <table class="table table-striped border">
            <tr class="table-info">
              

                <th>
                    @Html.DisplayNameFor(m => m.Name)

                </th>

                <th> </th>
            </tr>

    <div class="col-md-12">
        <table class="table table-striped border">


            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(m => item.Name)

                    </td>
                    <partial name="_TableButtonPartial" model="item.Id" />


                </tr>

            }

        </table>
    </div>
               

</body>
</html>





 


